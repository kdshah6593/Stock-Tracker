<table class="table table-hover">
    <thead class="thead-dark">
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Symbol</th>
        <th scope="col">Sector</th>
    </tr>
    </thead>
    <tbody>
    <% @stocks.each do |stock| %>
        <tr>
            <td><%= link_to stock.name, stock_path(stock) %></td>
            <td><%= stock.symbol %></td>
            <td><%= stock.sector %></td>
        </tr>
    <% end %>
    </tbody>
</table>